{
  "name": "ada-quizzes",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "eslintConfig": {
    "extends": [
      "get-off-my-lawn",
      "prettier"
    ],
    "rules": {
      "react/display-name": 0,
      "react/react-in-jsx-scope": 0,
      "react/jsx-no-literals": 0,
      "no-param-reassign": 0,
      "node/no-unpublished-require": 0,
      "jsx-a11y/anchor-is-valid": 0,
      "get-off-my-lawn/prefer-arrow-functions": 0,
      "no-unused-vars": 0,
      "no-warning-comments": 0,
      "no-console": 0
    }
  },
  "lint-staged": {
    "pages/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write",
    "components/**/*.{js,jsx,ts,tsx,json,css,scss,md}": "prettier --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@chakra-ui/react": "^1.3.3",
    "@emotion/react": "^11.1.5",
    "@emotion/styled": "^11.1.5",
    "framer-motion": "^3.6.2",
    "mongodb": "^3.6.4",
    "mongoose": "^5.11.17",
    "next": "10.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-icons": "^4.2.0",
    "react-router-dom": "^5.2.0",
    "swr": "^0.4.2"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-get-off-my-lawn": "^5.0.1",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  }
}
